# API Configuration
# Copy this file to .env.local and update values

# Backend API URL
VITE_API_URL=http://localhost:4000/api

# Customer frontend URL (for NFC card URLs)
VITE_CUSTOMER_URL=http://localhost:5172

# Default venue theme (default, fairmont, kimpton, ace, marriott)
VITE_DEFAULT_VENUE=default

# Debug mode
VITE_DEBUG=true
